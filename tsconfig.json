{
  "compilerOptions": {
    "moduleResolution": "node",
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "Components/*": ["src/Components/*"],
      "Pages/*": ["src/Pages/*"],
      "Redux/*": ["src/Redux/*"],
      "Layouts/*": ["src/Layouts/*"],
      "Configs/*": ["src/Configs/*"],
      "Routes/*": ["src/Routes/*"],
      "Helpers/*": ["src/Helpers/*"],
      "Assets/*": ["src/Assets/*"],
      "Constants/*": ["src/Constants/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
